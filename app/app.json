{
  "expo": {
    "name": "tsd-app2",
    "slug": "tsd-app-1",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon1.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gandaierama.tsdapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "ACCESS_FINE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "CALL_PHONE",
        "INTERNET",
        "MANAGE_MEDIA",
        "READ_CALENDAR",
        "READ_SMS",
        "RECEIVE_SMS",
        "SEND_SMS",
        "VIBRATE",
        "WRITE_CALENDAR"
      ],
      "package": "com.gandaierama.tsdapp"
    },
    "web": {
      "favicon": "./assets/icon1.png"
    },
    "plugins": [
      [
        "onesignal-expo-plugin",
        {
          "mode": "development"
        }
      ]
    ],
    "description": "App TSD"
  }
}
